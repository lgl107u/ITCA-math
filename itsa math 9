#include <iostream>

using namespace std;
int main()
{
    unsigned int x;
    while(cin >> x)
    {
        if(x>31) cout << "Value of more than 31\n" ;
        
        else
        {
            int l=1;
            unsigned long long resl;
            resl = (unsigned long long)l << x;
            cout << resl << endl;
        }
    }
    return 0;
}
